{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/_app.tsx", pages/index.js, "pages/discover.jsx", "pages/book/[id].js", "papages/middleware.tsx "pages/api/auth/[...nextauth2].ts",, "pages/components/sidebar.js" "pages/components/wrapper.js"],
  "exclude": ["node_modules"]
}
